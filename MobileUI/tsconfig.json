{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "react-native",
		"lib": [
			"es2017"
		],
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		"target": "esnext"
	},
	"extends": "expo/tsconfig.base.json",
	"include": [
		"src/**/*.ts",
		"src/**/*.tsx"
	],
	"exclude": [
		"node_modules",
		"babel.config.js",
		"metro.config.js",
		"jest.config.js"
	]
}